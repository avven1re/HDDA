k <- 1
for (i in 1 : length(covst$門市地址)) {
templl <- geoPoint(covst$門市地址[i], "AIzaSyD7ExFH62Ni2cYE5Vp4939DVO6DeUKxupk") #放入GoogleAPI金鑰
if(sum(templl[1] == "Status: ZERO_RESULTS") == 1){
llpos[i, 1] <- NA
llpos[i, 2] <- NA
cat("遺漏", k, "個", "\n")
k <- k + 1
next()
}
else{
llpos[i, 1] <- templl[1, 1]
llpos[i, 2] <- templl[1, 2]
cat("完成,", i, "個", "\n")}
}
